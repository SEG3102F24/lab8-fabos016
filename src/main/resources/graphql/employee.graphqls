type Query {
    employees: [Employee]
    employeesById(employeeId: ID!): [Employee]
}

type Employee {
    employeeId: ID!
    name: String!
    dateOfBirth: String !
    city: String!
    salary: Float!
    gender: String!
    email: String!
}

input CreateEmployeeInput {
    name: String!
    dateOfBirth: String !
    city: String!
    salary: Float!
    gender: String!
    email: String!
}

type Mutation {
    newEmployee(createEmployeeInput: CreateEmployeeInput!) : Employee!
}